function Book(title) {
    return [
		{ "title" : title },
		{ "sections" : [] },

		{ "AppendSection" :
			(function(book, section) {
				a = objecttotalmembers(book.sections);
//                print(a,"\n");
                print(book,"\n");
//                print(book.sections,"\n");
			})
		}
 
    ];
}

 

a = Book("Hello World");
