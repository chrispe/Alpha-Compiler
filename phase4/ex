function Book(title) {
	print("");
    return [
		{ "title" : title },
		{ "sections" : [] },

		{ "AppendSection" :
			(function(book, section) {
				a = objecttotalmembers(book.sections);
				book.sections[a] = section;
				//print(book);
			})
		
		}
 
    ];
}

 

a = Book("Hello World");
//a.sections = [];
//a..AppendSection("Section 1");
//a..AppendSection("Section 2");
print(a); 
